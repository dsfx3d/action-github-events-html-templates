    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "test": "jest",
    "coverage": "jest --coverage",
    "watch:template": "nodemon --exec ts-node -e tsx,ts template-dev/server.ts",
    "watch:css": "nodemon -e tsx scripts/generateCss.mjs",
    "watch": "pnpm run /^watch:.*$/",
    "storybook": "storybook dev -p 6006",
